{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "theme-editor",
  "description": "Capability for the theme editor window",
  "windows": [
    "theme-editor"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-close",
    "core:window:allow-destroy",
    "core:window:allow-set-focus",
    "core:event:default",
    "store:default"
  ]
}
