{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:app:allow-set-app-theme",
    "core:window:allow-set-theme",
    "sql:default",
    "sql:allow-execute",
    "core:window:default",
    "core:window:allow-start-dragging",
    "store:default",
    "dialog:default",
    "fs:allow-write-text-file",
    "os:default"
  ]
}